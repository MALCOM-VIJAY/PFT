#include<stdio.h>
int main(){
    int inp,nod=0,safe=0,modulo_value=0,multiple_t=1,final=0;
    scanf("%d",&inp);
    safe=inp;
    while(inp){
        inp=inp/10;
        nod+=1;
    }
    inp=safe;
    while(inp>0){
        modulo_value=inp%10;
        for(int i=0;i<nod;i++){
            multiple_t*=modulo_value;
        }
        final+=multiple_t;
        inp=inp/10;
        multiple_t=1;
    }
    inp=safe;
    if(inp==final){
        printf("It is an armstrong number");
    }
    else{
        printf("It is not an armstrong number");
    }
}

INPUT:
153
OUTPUT:
It is an armstrong number
