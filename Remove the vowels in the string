#include <stdio.h>
#include<string.h>
int main()
{
    char ch[]={"this is mvj"};
    int n=strlen(ch),j=0;
    for(int i=0;i<=n;i++){
          if(ch[i]!='a'&& ch[i]!='e'&&ch[i]!='i'&&ch[i]!='o'&&ch[i]!='u'&&ch[i]!='A'&&ch[i]!='E'&&ch[i]!='I'&&ch[i]!='O'&&ch[i]!='U'){
           ch[j]=ch[i];
           j++;
       }
    }
    printf("%s",ch);
    
    return 0;
}
