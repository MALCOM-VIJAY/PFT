    {
        for(ctr=1;ctr<=input-line;ctr+=1)
            printf(" ");
        for(ctr=1;ctr<=line*2-1;ctr+=1)
            printf("*");
        for(ctr=1;ctr<=(input-line)*2;ctr+=1)
            printf(" ");
